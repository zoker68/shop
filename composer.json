{
    "name": "zoker/shop",
    "description": "Shop script",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Zoker\\Shop\\": "src/",
            "Zoker\\Shop\\Database\\Seeders\\": "database/seeders/",
            "Zoker\\Shop\\Database\\Factories\\": "database/factories/"
        }
    },
    "authors": [
        {
            "name": "Maksim",
            "email": "zokermm@gmail.com",
            "homepage": "https://github.com/zoker"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.9",
        "livewire/livewire": "^3.0",
        "filament/filament": "^3.2",
        "filament/notifications": "^3.2",
        "laravel/scout": "^10.10",
        "meilisearch/meilisearch-php": "^1.9",
        "veelasky/laravel-hashid": "^3.1",
        "akaunting/laravel-money": "^5.2",
        "bkwld/croppa": "^7.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zoker\\Shop\\Providers\\ShopServiceProvider",
                "Zoker\\Shop\\Providers\\FilamentPanelProvider"
            ],
            "aliases": {
                "Debugbar": "Barryvdh\\Debugbar\\Facade"
            }
        }
    }
}
